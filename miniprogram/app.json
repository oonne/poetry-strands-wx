{
  "pages": [
    "pages/main/index"
  ],
  "subPackages": [
    {
      "root": "pages/game",
      "pages": [
        "index"
      ]
    },
    {
      "root": "pages/poetry-list",
      "pages": [
        "index"
      ]
    },
    {
      "root": "pages/setting",
      "pages": [
        "index"
      ]
    }
  ],
  "preloadRule": {
    "pages/main/index": {
      "network": "all",
      "packages": ["pages/game", "pages/poetry-list"]
    }
  },
  "window": {
    "navigationStyle": "custom"
  },
  "style": "v2",
  "renderer": "skyline",
  "rendererOptions": {
    "skyline": {
      "defaultDisplayBlock": true,
      "defaultContentBox": true,
      "tagNameStyleIsolation": "legacy",
      "disableABTest": true,
      "sdkVersionBegin": "3.0.0",
      "sdkVersionEnd": "15.255.255"
    }
  },
  "componentFramework": "glass-easel",
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents"
}